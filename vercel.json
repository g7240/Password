{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/public/(.*)",
            "dest": "/public/"
        },
        {
            "src": "/public/articoloPassword.pdf",
            "dest": "/public/articoloPassword.pdf"
        }
    ]
}
